{
  "name": "coyote-grill",
  "version": "1.0.0",
  "description": "Website and other promotional documents for Lil' Debbie's Coyote Grill",
  "main": "js/bundle.js",
  "scripts": {
    "start": "run-p watch:jekyll serve",
    "build": "run-s build:webpack build:jekyll",
    "build:jekyll": "bundle exec jekyll build",
    "build:webpack": "NODE_ENV=production webpack --mode production",
    "watch": "run-p watch:*",
    "watch:jekyll": "bundle exec jekyll build -w",
    "watch:webpack": "webpack --mode development --progress --color --watch",
    "serve": "webpack-dev-server --mode development --hot",
    "test": "echo \"Error: no test specified\" && exit 1",
    "menu": "google-chrome --headless --disable-gpu --print-to-pdf http://localhost:3000/menu && mv output.pdf menu.pdf",
    "analyze:bundle": "webpack-bundle-analyzer js/stats.json"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "color": "^3.1.0",
    "firebase": "^7.13.1",
    "flickity": "^2.1.2",
    "normalize.css": "^8.0.1",
    "preact": "^10.3.4",
    "proptypes": "^1.1.0",
    "rollbar": "^2.15.0",
    "typeface-lato": "^0.0.75",
    "typeface-oswald": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-object-values-to-object-keys": "^1.0.0",
    "browser-sync": "^2.26.3",
    "core-js": "3",
    "css-loader": "^3.4.2",
    "css-toggle-switch": "^4.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-react": "^7.19.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "imports-loader": "^0.8.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.1.5",
    "raf": "^3.4.1",
    "raw-loader": "^4.0.0",
    "style-loader": "^1.1.3",
    "styled-jsx": "^3.2.5",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^3.0.0",
    "svelte": "^2.16.1",
    "svelte-loader": "^2.12.0",
    "svelte-upgrade": "^1.0.16",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yaml-loader": "^0.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobmischka/coyote-grill.git"
  },
